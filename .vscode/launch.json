{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "cemindmap_ui_local",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define=PROJECTS_BASE_URL=http://localhost:8888/",
        "--dart-define=ASSIGNMENTS_BASE_URL=http://localhost:8888/"
      ]
    },
    {
      "name": "cemindmap_ui_cirrus",
      "request": "launch",
      "type": "dart",
      "args": [
        "--dart-define=PROJECTS_BASE_URL=https://cebrowserapi.dal1a.ciocloud.nonprod.intranet.ibm.com/",
        "--dart-define=ASSIGNMENTS_BASE_URL=https://cebrowserapi.dal1a.ciocloud.nonprod.intranet.ibm.com/"
      ]
    },
    {
      "name": "cemindmap_ui (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "cemindmap_ui (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}